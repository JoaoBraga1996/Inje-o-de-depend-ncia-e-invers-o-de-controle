package application;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {

		List<String> list = new ArrayList<>();

		list.add("Ana");
		list.add("Luiz");
		list.add("Roberto");
		list.add("Cadu");

		list.add(1, "Jo√£o");

		for (String elem : list) {
			System.out.println(elem);
		}
		
		list.remove(0);
		
		for (String elem : list) {
			System.out.println(elem);
		}
		
		

	}
}
